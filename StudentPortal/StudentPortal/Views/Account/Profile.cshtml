@model StudentPortal.Models.Profile
@{
    ViewBag.Title = "Profile";
}

<h2>@ViewBag.Title.</h2>


<div class="form-group">
    <div class="col-md-10">
        <label>StudentId : </label>  @Html.DisplayFor(m => m.StudentId)
    </div>
</div>
<div class="form-group">
    <div class="col-md-10">
        <label>Email : </label>  @Html.DisplayFor(m => m.Email)
    </div>
</div>
<div class="form-group">
    <div class="col-md-10">
        <label>FirstName : </label>  @Html.DisplayFor(m => m.FirstName)
    </div>
</div>
<div class="form-group">
    <div class="col-md-10">
        <label>LastName : </label>  @Html.DisplayFor(m => m.LastName)
    </div>
</div>
<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <a href="/Account/EditProfile" class="btn btn-default">Edit Profile</a>
    </div>
</div>


