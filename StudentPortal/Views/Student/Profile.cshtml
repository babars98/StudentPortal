@model StudentPortal.Models.Profile
@{
    ViewBag.Title = "Profile";
}

<h2>@ViewBag.Title.</h2>

<table class="table">
    <tr>
        <td>StudentId</td>
        <td>@Model.StudentId</td>
    </tr>
    <tr>
        <td>Email</td>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <td>First Name</td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Last Name</td>
        <td>@Model.LastName</td>
    </tr>
</table>
<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <a href="/Student/EditProfile" class="btn btn-primary">Edit Profile</a>
    </div>
</div>


